{
    "body": "{%- trans_default_domain trans_default_domain -%}\n{% extends '@EMSCH/template/fragments/abstract.twig' %}\n\n{%- block content -%}\n    {% apply spaceless %}\n        <div class=\"container d-print-none\">\n            <div class=\"my-3\">\n                <div class=\"h4\">{{ 'website.name'|trans }}</div>\n                <div class=\"footer-nav\">\n                    <nav role=\"navigation\">\n                        <ul class=\"nav justify-content-center\">\n                            {% for social in socials %}\n                                <li class=\"nav-item\">\n                                    <a class=\"nav-link\" href=\"{{ social.url }}\" title=\"{{ social.name }}\">\n                                        <i class=\"{{ social.icon }}\"></i>\n                                        <span class=\"menu-title sr-only\">{{ social.name }}</span>\n                                    </a>\n                                </li>\n                            {% endfor %}\n                        </ul>\n                    </nav>\n                </div>\n            </div>\n            <div class=\"text-small\">\n                <div class=\"mb-1\">\u00a9 {{ 'now'|date('Y') }}. All rights reserved.</div>\n            </div>\n        </div>\n    {% endapply %}\n{%- endblock content -%}\n",
